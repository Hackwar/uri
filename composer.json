{
    "name": "joomla/uri",
    "type": "joomla-package",
    "description": "Joomla Uri Package",
    "keywords": ["joomla", "framework", "uri"],
    "homepage": "https://github.com/joomla-framework/uri",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
		"psr/http-message": "~1.0"
    },
    "require-dev": {
	    "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Uri\\": "src/",
            "Joomla\\Uri\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    }
}
